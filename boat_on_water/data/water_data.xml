<odoo>
    <data noupdate="0">
        <record id="sequence_water_contract" model="ir.sequence">
            <field name="name">Water Contract</field>
            <field name="code">water.contract</field>
            <field name="prefix">SW/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
        <!-- Water Storage product -->
        <record id="product_product_water_storage" model="product.product">
            <field name="name">Water Storage</field>
            <field name="default_code">Storage_W01</field>
            <field name="type">service</field>
            <field name="sale_ok" eval="False"/>
            <field name="purchase_ok" eval="False"/>
            <field name="list_price">0.0</field>
        </record>

    </data>
</odoo>