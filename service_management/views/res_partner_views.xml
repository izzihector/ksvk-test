<odoo>
    <record id="view_res_partner_service_inherit_form" model="ir.ui.view">
        <field name="name">Partner Form inherit Service Management</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="Nettivene.view_res_partner_inherit_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='nettix_info']" position="inside">
                <group>
                    <group>
                        <field name="boat_make_id"/>
                        <field name="boat_model"/>
                        <field name="boat_length"/>
                        <field name="boat_width"/>
                        <field name="year"/>
                    </group>
                    <group>
                        <field name="engine_model"/>
                        <field name="engine_hours"/>
                        <field name="engine_mfg_year"/>
                    </group>
                </group>
            </xpath>
        </field>
    </record>
</odoo>
